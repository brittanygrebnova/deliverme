<h2>Edit Your Account Info</h2>

<%= form_for @user do |f| %>
  <%= f.label :street_address %>: <%= f.text_field :street_address %>
  <%= f.label :city %>: <%= f.text_field :city %>
  <%= f.label :state %> <%= f.select :state, options_for_select(us_states) %><br>
  <%= f.submit "Edit Info" %>
<% end %>

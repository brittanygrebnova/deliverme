<%= link_to "Back To Home", user_path(current_user) %>

<div>
  <h2>Place a New Order with <%= @vendor.name %></h2>
  <%= form_for @order do |f| %>
    <%= f.hidden_field :vendor_id %>
    Choose Your Item(s ):<br>
    <%= f.collection_check_boxes(:item_ids, @items, :id, :name) %><br>
    <%= f.submit "Place Order"%>
  <% end %>
</div>
